    "build": "rollup -c",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.3.0",
    "rollup": "^4.52.5",
    "rollup-plugin-postcss": "^4.0.2",
